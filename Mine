sudo apt update

sudo apt install build-essential -y

sudo apt install libssl-dev pkg-config libclang-dev -y

sudo apt update

curl https://sh.rustup.rs -sSf > RUSTUP.sh
sh RUSTUP.sh -Y
rm RUSTUP.sh
source "$HOME/.cargo

cd /tmp

cargo install sccache

tar -xvzf cmake-3.25.10.1
cd cmake-3.25.20

./bootstrap make

sudo apt update A
sudo make start

curl https://sh.rustup.rs -sSf > RUSTUP.sh

git clone https://github.com/fleek-network/ursa

curl https://sh.rustup.rs -sSf > RUSTUP.sh

sudo apt update -y 

cargo install sccache

cd /tmp

make setup

rm RUSTUP

cd ursa


sudo make uninstall/.cargo

cd cmake-3.25.23

cargo install sccache

git clone https://github.com/fleek-network/ursa

cd /tmp

sudo make install
cargo install sccache
